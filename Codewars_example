// first task
function points(games) {
  let sum=0;
  for (let i=0; i<games.length; ++i)
  {
    if (games[i][0]>games[i][2])
      sum+=3;
    if (games[i][0]==games[i][2])
      sum+=1;
  }
  return sum;
}

//  create your first JS function and print "Hello World!"
function helloWorld () {
  var str='Hello World!';
  console.log(str);
}
